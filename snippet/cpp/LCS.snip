  void LIS(string s,string t){
    FOR(i,1,s.size()+1){
      FOR(j,1,t.size()+1){
        if(s[i-1]==t[j-1]){
          if(i==0||j==0){
            dp[i][j]=1;
          }
          else{
            dp[i][j] = dp[i-1][j-1]+1;
          }
        }
        dp[i][j] = max(dp[i][j],dp[i-1][j]);
        dp[i][j] = max(dp[i][j],dp[i][j-1]);
      }
    }
  }
