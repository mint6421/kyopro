  void tsort_dfs(int u,vector<int> &graph,bool visited[],UG &g){

    if(visited[u]) return;
    visited[u] = true;
    for(int i=0;i<g[u].size();i++){
      tsort_dfs(g[u][i].to,graph,visited,g);
    }
    graph.PB(u);
  }

  vector<int> tsort(UG &g){
    vector<int> graph;
    bool visited[110000] = {};
    for(int i=0;i<g.size();i++){
      tsort_dfs(i,graph,visited,g);
    }
    reverse(all(graph));
    return graph;
  }


